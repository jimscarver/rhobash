// A simple HelloJimscarver contract demonstrating a contract being rerun

new  stdout(`rho:io:stdout`) in {
    contract @"hellojimscarver"( jimscarver ) = {
        for( @msg <- jimscarver ) {
            stdout!(msg)
        }
    } |
    new jimscarver, jimscarver2 in {
        @"hellojimscarver"!(*jimscarver) |
        jimscarver!("Hello Jimscarver") |
        @"hellojimscarver"!(*jimscarver2) |
        jimscarver2!("Hello Jimscarver again")
    }
}
